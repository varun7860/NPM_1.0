<?xml version="1.0" encoding="utf-8" ?>
<robot name="npm">
    <!-- Controller Plugin for gazebo -->
    <gazebo>
        <plugin filename="libgazebo_ros_control.so" name="gazebo_ros_control">
            <robotNamespace>/</robotNamespace>
            <!--<robotSimType>gazebo_ros_control/DefaultRobotHWSim</robotSimType>
        <legacyModeNS>true</legacyModeNS>-->
        </plugin>
    </gazebo>
    <!--Gazebo Colours -->
    <gazebo reference="base">
        <material>Gazebo/Blue</material>
    </gazebo>
    <gazebo reference="link11">
        <material>Gazebo/Red</material>
    </gazebo>
    <gazebo reference="link12">
        <material>Gazebo/Green</material>
    </gazebo>
    <gazebo reference="link13">
        <material>Gazebo/Red</material>
    </gazebo>
    <gazebo reference="link21">
        <material>Gazebo/Green</material>
    </gazebo>
    <gazebo reference="link22">
        <material>Gazebo/White</material>
    </gazebo>
    <gazebo reference="link3">
        <material>Gazebo/Black</material>
    </gazebo>
    <gazebo reference="link4">
        <material>Gazebo/White</material>
    </gazebo>
    <gazebo reference="link5">
        <material>Gazebo/Purple</material>
    </gazebo>
    <gazebo reference="link6">
        <material>Gazebo/Purple</material>
    </gazebo>
    <gazebo reference="link7">
        <material>Gazebo/Purple</material>
    </gazebo>

    <!--Make the base of the Robot -->
    <link name="world" />
    <link name="base">
        <visual name="base_visual">
            <origin rpy="0 0 0" xyz="0 0 0.05" />
            <geometry>
                <box size="0.2 0.2 0.1" />
            </geometry>
        </visual>
        
<inertial>
            <mass value="1" />
            <origin rpy="0 0 0" xyz="0 0 0.05" />
            <inertia ixx="0.004" ixy="0" ixz="0" iyy="0.004" iyz="0" izz="0.006" />
        </inertial>

        <collision name="collision_base">
            <origin rpy="0 0 0" xyz="0 0 0.05" />
            <geometry>
                <box size="0.2 0.2 0.1" />
            </geometry>
        </collision>
    </link>
    <!-- Fix the base to the world -->
    <joint name="base_joint" type="fixed">
        <parent link="world" />
        <child link="base" />
    </joint>
    <!-- link_1_1 -->
    <link name="link11">
        <collision name="link11_collision">
            <origin rpy="0 0 0" xyz="0 0 0.3" />
            <geometry>
                <cylinder length="0.4" radius="0.05" />
            </geometry>
        </collision>
        
<inertial>
            <mass value="1" />
            <origin rpy="0 0 0" xyz="0 0 0.3" />
            <inertia ixx="0.013958" ixy="0" ixz="0" iyy="0.013958" iyz="0" izz="0.00125" />
        </inertial>

        <visual name="link11_visual">
            <origin rpy="0 0 0" xyz="0 0 0.3" />
            <geometry>
                <cylinder length="0.4" radius="0.05" />
            </geometry>
        </visual>
    </link>
    <!-- Joint_base_link11 Type -->
    <joint name="revolute_joint_1" type="continuous">
        <parent link="base" />
        <child link="link11" />
        <axis xyz="0 0 1" />
        <origin rpy="0 0 0" xyz="0 0 0" />
        <dynamics damping="0.1" />
    </joint>
    <!--Fixed link_12 -->
    <link name="link12">
        <collision name="link12_fixed_collision">
            <origin rpy="0 0 0" xyz="0.2 0 0.5" />
            <geometry>
                <box size="0.4 0.1 0.05" />
            </geometry>
        </collision>
        
        <inertial>
            <mass value="1" />
            <origin rpy="0 0 0" xyz="0.2 0 0.5" />
            <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.135" iyz="0" izz="0.0141" />
        </inertial>

        <visual name="link12_fixed_visual">
            <origin rpy="0 0 0" xyz="0.2 0 0.5" />
            <geometry>
                <box size="0.4 0.1 0.05" />
            </geometry>
        </visual>
    </link>
    <!-- Joint_1_2 Type -->
    <joint name="Joint12" type="fixed">
        <parent link="link11" />
        <child link="link12" />
    </joint>

    <!--Link_1_3 -->
    <link name="link13">
        <collision name="link13_collision">
            <origin rpy="0 0 0" xyz="0.4 0 0.525" />
            <geometry>
                <cylinder length="0.1" radius="0.05" />
            </geometry>
        </collision>
        
<inertial>
            <mass value="1" />
            <origin rpy="0 0 0" xyz="0.4 0 0.525" />
            <inertia ixx="0.0025" ixy="0" ixz="0" iyy="0.0025" iyz="0" izz="0.00125" />
        </inertial>

        <visual name="link13_visual">
            <origin rpy="0 0 0" xyz="0.4 0 0.525" />
            <geometry>
                <cylinder length="0.1" radius="0.05" />
            </geometry>
        </visual>
    </link>
    <!-- Joint13 -->
    <joint name="Joint13" type="fixed">
        <parent link="link12" />
        <child link="link13" />
    </joint>
    <!--link_2_1 -->
    <link name="link21">
        <collision name="link21_fixed_1_collision_1">
            <origin rpy="0 0 0" xyz="0.2 0 0.05" />
            <geometry>
                <box size="0.4 0.1 0.05" />
            </geometry>
        </collision>
        
        <inertial>
            <mass value="0.5" />
            <origin rpy="0 0 0" xyz="0.2 0 0.05" />
            <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.0135" iyz="0" izz="0.0141" />
        </inertial>

        <visual name="link21_fixed_1_visual_1">
            <origin rpy="0 0 0" xyz="0.2 0 0.05" />
            <geometry>
                <box size="0.4 0.1 0.05" />
            </geometry>
        </visual>
    </link>

    <!-- Create revolute joint 2 -->
    <joint name="revolute_joint_2" type="continuous">
        <parent link="link13" />
        <child link="link21" />
        <axis xyz="0 0 1" />
        <origin rpy="0 0 0" xyz="0.4 0 0.5" />
        <dynamics damping="0.1" />
    </joint>
    <!--Fixed link for link_2 -->
    <link name="link22">
        <collision name="link22_fixed_2_collision_2">
            <origin rpy="0 0 0" xyz="0.4 0 0.05" />
            <geometry>
                <sphere radius="0.05" />
            </geometry>
        </collision>
        
<inertial>
            <mass value="1" />
            <origin rpy="0 0 0" xyz="0.4 0 0.05" />
            <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001" />
        </inertial>

        <visual name="link22_fixed_2_visual_2">
            <origin rpy="0 0 0" xyz="0.4 0 0.05" />
            <geometry>
                <sphere radius="0.05" />
            </geometry>
        </visual>
    </link>
    <!--Fixed joint for link_2_1_2 -->
    <joint name="joint22" type="fixed">
        <parent link="link21" />
        <child link="link22" />
    </joint>
    <!-- Link 3 -->
    <link name="link3">
        <collision name="link3_collision">
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.3" radius="0.03" />
            </geometry>
        </collision>
        
        <inertial>
            <mass value="1" />
            <origin rpy="0 0 0" xyz="0 0 0" />
            <inertia ixx="0.025225" ixy="0" ixz="0" iyy="0.025225" iyz="0" izz="0.00045" />
        </inertial>

        <visual name="link3_visual">
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.3" radius="0.03" />
            </geometry>
        </visual>
    </link>
    <!-- Joint 3 -->
    <joint name="joint_3" type="prismatic">
        <child link="link3" />
        <parent link="link22" />
        <origin rpy="0 0 0" xyz="0.4 0 0" />
        <axis xyz="0 0 1" />
        <limit lower="-0.1" upper="0.12" effort="1000" velocity="0.1" />
        <dynamics damping="0.4" friction="15" />
    </joint>

    <link name="link4">
        <collision name="link4_collision">
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.02" radius="0.05" />
            </geometry>
        </collision>
        
        <inertial>
            <mass value="0.02" />
            <origin rpy="0 0 0" xyz="0 0 0" />
             <inertia ixx="2.29166666666667e-05" ixy="0" ixz="0" iyy="2.29166666666667e-05" iyz="0" izz="1.25e-05"/>
        </inertial>

        <visual name="link4_visual">
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.02" radius="0.05" />
            </geometry>
        </visual>
    </link>

    <!-- Joint 4 -->
    <joint name="joint_4" type="continuous">
        <child link="link4" />
        <parent link="link3" />
        <origin rpy="0 0 0" xyz="0.0 0 -0.15" />
        <axis xyz="0 0 1" />
        <dynamics damping="0.2" friction="0.2" />
    </joint>


    <link name="link5">
        <collision name="link5_collision">
            <origin rpy="1.57 0 0" xyz="0 0 -0.02" />
            <geometry>
                <cylinder length="0.13" radius="0.01" />
            </geometry>
        </collision>
        
        <inertial>
            <mass value="0.02" />
            <origin rpy="1.57 0 0" xyz="0 0 -0.02" />
            <inertia ixx="0.000217166666666667" ixy="0" ixz="0" iyy="0.000217166666666667" iyz="0" izz="1e-06" />
        </inertial>

        <visual name="link5_visual">
            <origin rpy="1.57 0 0" xyz="0 0 -0.02" />
            <geometry>
                <cylinder length="0.13" radius="0.01" />
            </geometry>
        </visual>
    </link>
    <!-- Joint 5 -->
    <joint name="joint_5" type="fixed">
        <child link="link5" />
        <parent link="link4" />
    </joint>


    <link name="link6">
        <collision name="link6_collision">
            <origin rpy="0 0 0" xyz="0.07 -0.058 -0.025" />
            <geometry>
                <box size="0.15 0.01 0.03"/>
            </geometry>
        </collision>
        
        <inertial>
            <mass value="0.02" />
            <origin rpy="0 0 0" xyz="0.07 -0.058 -0.025" />
            <inertia ixx="0.000037667" ixy="0" ixz="0" iyy="0.0000016" iyz="0" izz="0.000039" />
        </inertial>

        <visual name="link6_visual">
            <origin rpy="0 0 0" xyz="0.07 -0.058 -0.025" />
            <geometry>
                <box size="0.15 0.01 0.03"/>
            </geometry>
        </visual>
    </link>
    <!-- Joint 6 -->
    <joint name="joint_6" type="fixed">
        <child link="link6" />
        <parent link="link5" />
    </joint>


     <link name="link7">
        <collision name="link7_collision">
            <origin rpy="0 0 0" xyz="0.07 0.058 -0.025" />
            <geometry>
                <box size="0.15 0.01 0.03"/>
            </geometry>
        </collision>
        
        <inertial>
            <mass value="0.02" />
            <origin rpy="0 0 0" xyz="0.07 0.058 -0.025" />
            <inertia ixx="0.000037667" ixy="0" ixz="0" iyy="0.0000016" iyz="0" izz="0.000039" />
        </inertial>

        <visual name="link7_visual">
            <origin rpy="0 0 0" xyz="0.07 0.058 -0.025" />
            <geometry>
                <box size="0.15 0.01 0.03"/>
            </geometry>
        </visual>
    </link>
    <!-- Joint 6 -->
    <joint name="joint_7" type="fixed">
        <child link="link7" />
        <parent link="link5" />
    </joint>
    
    
<transmission name="trans_revolute_joint_1">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="revolute_joint_1">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        </joint>
        <actuator name="motor1">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
    <transmission name="trans_revolute_joint_2">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="revolute_joint_2">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        </joint>
        <actuator name="motor2">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
    <transmission name="trans_joint_3">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="joint_3">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        </joint>
        <actuator name="motor3">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>

    <transmission name="trans_joint_4">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="joint_4">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        </joint>
        <actuator name="motor4">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>

    <gazebo>
        <plugin name="joint_state_publisher" filename="libgazebo_ros_joint_state_publisher.so">
            <jointName>revolute_joint_1, revolute_joint_2, joint_3, joint_4</jointName>
        </plugin>
    </gazebo>
 
</robot>
